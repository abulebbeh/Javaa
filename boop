import java.util.*;

public class RemoveNumber {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        int num;
        String val;
        ArrayList list = new ArrayList();
        System.out.println("Enter integers or ZZZ to quit");
        while (true) {
            val = input.next();
            if (val.equals("ZZZ")) {
                break;
            }
            num = Integer.parseInt(val);
            list.add(num);
        }
        Iterator itez = list.iterator();

        System.out.println("Number:");
        while (itez.hasNext()) {
            System.out.print(itez.next());
            System.out.print(" ");
        }
        System.out.println(" ");
        System.out.println("Enter number to ELiminate>>");
        num = input.nextInt();
        itez = list.iterator();
        while (itez.hasNext()) {
            if (itez.next() == num) {
                itez.remove();
            }
        }
        itez = list.iterator();
        System.out.println("Numbers:");
        while (itez.hasNext()) {
            System.out.print(itez.next());
            System.out.print(" ");
        }
    }
}
